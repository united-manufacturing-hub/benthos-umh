{
  "name": "Benthos Plugin",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "NODE_VERSION": "16",
      "VARIANT": "1"
    }
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "EditorConfig.EditorConfig",
    "esbenp.prettier-vscode"
  ],
  "postCreateCommand": "npm install",
  "forwardPorts": [3000, 3002],
  "remoteUser": "vscode"
}
